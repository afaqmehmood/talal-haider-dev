{% comment %}
DO NOT EDIT THIS FILE

This file defines theme bindings for the SALVO theme compilation framework.
{% endcomment %}

<script>
{% assign template_json = template | json | replace: '\/', '/' %}
{% if template_json == 'null' %}
window.SALVO_BINDINGS = {
    template: "page",
};
{% else %}
window.SALVO_BINDINGS = {
    template: {{ template_json }},
};
{% endif %}
</script>
{{ 'theme.js' | asset_url | script_tag }}
